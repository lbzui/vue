{
  "name": "@lbzui/vue",
  "version": "0.1.0",
  "private": false,
  "description": "Material Components for Vue.js",
  "author": "LAN Bizhong <bizhong_lan@163.com> (https://lanbizhong.com/)",
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "lint": "vue-cli-service lint",
    "build:doc": "vue-cli-service build",
    "build:lib": "vue-cli-service build --target lib --name lbzui --dest lib src/components/index.ts",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit"
  },
  "main": "lib/lbzui.common.js",
  "unpkg": "lib/lbzui.umd.min.js",
  "files": [
    "lib",
    "src"
  ],
  "dependencies": {
    "core-js": "^2.6.5",
    "vue": "^2.6.10",
    "vue-class-component": "^7.0.2",
    "vue-property-decorator": "^8.1.0",
    "vue-router": "^3.0.3",
    "vue-scroll": "^2.1.12"
  },
  "devDependencies": {
    "@types/jest": "^23.1.4",
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-e2e-cypress": "^3.11.0",
    "@vue/cli-plugin-typescript": "^3.11.0",
    "@vue/cli-plugin-unit-jest": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "mem": "^5.1.1",
    "normalize.css": "^8.0.1",
    "postcss-preset-env": "^6.7.0",
    "postcss-pxtorem": "^4.0.1",
    "style-resources-loader": "^1.2.1",
    "ts-jest": "^23.0.0",
    "typescript": "^3.4.3",
    "vue-cli-plugin-style-resources-loader": "^0.1.3",
    "vue-template-compiler": "^2.6.10"
  },
  "postcss": {
    "plugins": {
      "postcss-preset-env": {
        "stage": 3,
        "features": {},
        "autoprefixer": {
          "flexbox": "no-2009",
          "grid": "no-autoplace"
        },
        "preserve": true,
        "importFrom": ["src/assets/css/variables/variables.css"]
      },
      "postcss-pxtorem": {
        "rootValue": 16,
        "unitPrecision": 8,
        "propList": [
          "font",
          "font-size",
          "line-height"
        ],
        "selectorBlackList": [],
        "replace": true,
        "mediaQuery": false,
        "minPixelValue": 0
      }
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie < 10"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue",
      "ts",
      "tsx"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.tsx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/",
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "globals": {
      "ts-jest": {
        "babelConfig": true
      }
    }
  },
  "bugs": {
    "url": "https://github.com/lbzui/lbzui-vue/issues",
    "email": "lbz_ui@163.com"
  },
  "engines": {
    "node": ">=8.11.0"
  },
  "homepage": "https://github.com/lbzui/lbzui-vue#readme",
  "keywords": [
    "vue",
    "less",
    "postcss",
    "typescript",
    "material-components",
    "material-components-vue",
    "material-design",
    "lbzui",
    "lbzui-vue"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lbzui/lbzui-vue.git"
  },
  "style": "lib/lbzui.css",
  "umd:main": "lib/lbzui.umd.js"
}
